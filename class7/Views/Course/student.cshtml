
@{
    ViewData["Title"] = "student";
}

@if (ViewBag.err)
{
    <h1 class="alert alert-danger">@ViewBag.msg</h1>
}
else
{
    <h1 class="alert alert-primary">Student information details</h1>
    <br />
    <h1 class="alert alert-danger">@ViewBag.or</h1>
    <br />
    <h1 class="alert alert-primary">@ViewBag.na</h1>
    <br />
    <h1 class="alert alert-primary">@ViewBag.batc</h1>
    <br />


}
